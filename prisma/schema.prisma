generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model credentials {
  id        Int    @id @default(autoincrement())
  title     String
  url       String
  userLogin String
  password  String
}
